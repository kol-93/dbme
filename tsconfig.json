{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2017",
    "inlineSourceMap": true,
    "outDir": "distribution",
    "noImplicitAny": false,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  },
  "include": [
    "typings/**/*.d.ts",
    "source/**/*.ts"
  ],
  "types": [
  ],
  "exclude": [
    "node_modules"
  ]
}
